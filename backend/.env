# ============================
# FastAPI Configuration
# ============================
APP_NAME="Shooting Star V16 & V17 AI Engine"
APP_VERSION="17.0.0"
DEBUG=false

# ============================
# Database Configuration
# ============================
DATABASE_URL="postgresql+asyncpg://username:password@localhost:5432/shooting_star"

# ============================
# JWT Configuration
# ============================
SECRET_KEY="your-super-secret-jwt-key-here"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30

# ============================
# Redis Configuration
# ============================
REDIS_URL="redis://localhost:6379/0"
CELERY_BROKER_URL="redis://localhost:6379/0"
CELERY_RESULT_BACKEND="redis://localhost:6379/0"

# ============================
# V16 AI Engine Configuration
# ============================
AI_ENGINE_ENABLED=true
AI_MODEL_PATH="models/v16/"
OPENAI_API_KEY="your-openai-api-key"
HUGGINGFACE_TOKEN="your-huggingface-token"

AI_MODEL="gpt-4"
SENTIMENT_MODEL="cardiffnlp/twitter-roberta-base-sentiment-latest"
GROWTH_PREDICTION_MODEL="facebook/prophet"

ENABLE_REALTIME_ANALYTICS=true
ANALYTICS_UPDATE_INTERVAL=300

AI_MAX_BUDGET_RECOMMENDATION=10000.00
REQUIRE_HUMAN_APPROVAL=true

AI_PREDICTION_TIMEOUT=30
MAX_CONCURRENT_AI_TASKS=10

# ============================
# V17 AI Engine Configuration
# ============================
V17_AI_ENGINE_ENABLED=true
V17_AI_CLUSTER_SIZE=3
V17_AI_MAX_NODES=50
V17_AI_PREDICTIVE_SCALING=true

V17_AI_GOVERNANCE_ENABLED=true
V17_AI_COMPLIANCE_FRAMEWORKS='["GDPR", "HIPAA"]'

V17_MICROSERVICES_ENABLED=true
V17_ORCHESTRATION_ENABLED=true
V17_LOAD_BALANCING_ENABLED=true

V17_VECTOR_CACHE_ENABLED=true
V17_CACHE_MAX_MEMORY_MB=1024
V17_CACHE_DEFAULT_TTL=3600

V17_MULTI_MODAL_FUSION_ENABLED=true
V17_CAUSAL_INFERENCE_ENABLED=true
V17_EXPLAINABLE_AI_ENABLED=true
V17_TRANSFER_LEARNING_ENABLED=true

V17_REAL_TIME_LEARNING_ENABLED=true
V17_ACTIVE_LEARNING_ENABLED=true
V17_DRIFT_DETECTION_ENABLED=true

V17_AUTO_SCALING_ENABLED=true
V17_PREDICTIVE_SCALING_ENABLED=true
V17_SCALING_COOLDOWN=300
V17_TARGET_UTILIZATION=0.75

V17_AI_MONITORING_ENABLED=true
V17_PERFORMANCE_TRACKING_ENABLED=true
V17_COST_OPTIMIZATION_ENABLED=true

V17_AI_AUDIT_TRAIL_ENABLED=true
V17_MODEL_RISK_ASSESSMENT_ENABLED=true
V17_ETHICAL_AI_FRAMEWORKS='["fairness", "transparency", "privacy"]'